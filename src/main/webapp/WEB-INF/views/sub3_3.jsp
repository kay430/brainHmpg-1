function multi_sel_parse(s_v, num, val){
	data_arr = new Array();

	var op_str = '<option value="">선택하세요.</option>';

	if(val){
		for(var i = 0; i < 9; i++){
			if(i != num) continue;

			if(num > 1){
				var tmp_val = '';
				for(var ti = 0; ti < num-1; ti++){
					if(document.getElementsByName('multi'+s_v+'_'+ti).length && document.getElementsByName('multi'+s_v+'_'+ti)[0].value){
						tmp_val += document.getElementsByName('multi'+s_v+'_'+ti)[0].value;
					}
				}

				val = tmp_val + val;
			}


			var arr_chk = typeof data_arr[s_v][i];
			if(arr_chk != 'object') continue;
			var arr_key = Object.keys(data_arr[s_v][i]);

			//console.log(val);

			for(var j = 0; j < arr_key.length; j++){
				var tmp_k = arr_key[j];
				//console.log(tmp_k);
				if(tmp_k != val) continue;
				var tmp_data = data_arr[s_v][i][tmp_k];

				for(var k = 0; k < tmp_data.length; k++){
					if(!tmp_data[k]) continue;
					op_str += '<option value="'+tmp_data[k]+'">'+tmp_data[k]+'</option>';
				}
			}
		}
	}

	if(document.getElementsByName('multi'+s_v+'_'+num).length) document.getElementsByName('multi'+s_v+'_'+num)[0].innerHTML = op_str;

	var chk_val = '';
	for(var vi = 0; vi < 9; vi++){
		if(!document.getElementsByName('multi'+s_v+'_'+vi).length) continue;
		tmp_val = document.getElementsByName('multi'+s_v+'_'+vi)[0].value;

		//console.log(tmp_val);

		if(!tmp_val){
			if(!val){
				for(var vi2 = vi+1; vi2 < 9; vi2++){
					if(document.getElementsByName('multi'+s_v+'_'+vi2).length){
						document.getElementsByName('multi'+s_v+'_'+vi2)[0].value = '';
						document.getElementsByName('multi'+s_v+'_'+vi2)[0].innerHTML = '<option value="">선택하세요.</option>';
					}
				}
			}
			break;
		}

		if(chk_val) chk_val += ' 》';
		 chk_val += tmp_val;
	}

	if(document.getElementsByName('s_v'+s_v).length){
		document.getElementsByName('s_v'+s_v)[0].value = chk_val;
	}
}


